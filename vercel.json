{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/"
    },
    {
      "src": "/about",
      "dest": "/about"
    },
    {
      "src": "/about/",
      "dest": "/about"
    },
    {
      "src": "/services",
      "dest": "/services"
    },
    {
      "src": "/services/",
      "dest": "/services"
    },
    {
      "src": "/team",
      "dest": "/team"
    },
    {
      "src": "/team/",
      "dest": "/team"
    },
    {
      "src": "/projects",
      "dest": "/projects"
    },
    {
      "src": "/projects/",
      "dest": "/projects"
    },
    {
      "src": "/contact",
      "dest": "/contact"
    },
    {
      "src": "/contact/",
      "dest": "/contact"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/_next/static/(.*)",
      "dest": "/_next/static/$1"
    },
    {
      "src": "/_next/image(.*)",
      "dest": "/_next/image$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "/(.*)",
      "dest": "/404"
    }
  ],

  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    },
    {
      "source": "/_next/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/home",
      "destination": "/",
      "permanent": true
    }
  ]
}
